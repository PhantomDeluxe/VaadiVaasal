<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
    <display-name>VaadiVaasal_webapp</display-name>
    <welcome-file-list>
        <welcome-file>Jallikattu.jsp</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>PlayerServlet</servlet-name>
        <servlet-class>org.example.Player_Card.PlayerServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>PlayerServlet</servlet-name>
        <url-pattern>/PlayerServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UpdatePlayerServlet</servlet-name>
        <servlet-class>org.example.Player_Card.UpdatePlayerServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>UpdatePlayerServlet</servlet-name>
        <url-pattern>/UpdatePlayerServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AdminRegisterServlet</servlet-name>
        <servlet-class>org.example.Match_Schedule.AdminDatabase</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AdminRegisterServlet</servlet-name>
        <url-pattern>/AdminDatabase</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AdminLogin</servlet-name>
        <servlet-class>org.example.Match_Schedule.AdminLogin</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>AdminLogin</servlet-name>
        <url-pattern>/AdminLoginServlet</url-pattern>
    </servlet-mapping>

</web-app>
